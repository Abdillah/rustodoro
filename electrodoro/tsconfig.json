        "target": "es3",
        "module": "CommonJS",
        "moduleResolution": "Node",
        "jsx": "react",
        "jsxFactory": "element",
        "declaration": true,
        "outDir": "lib/view/script",
        "strict": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true
    },
    "include": [ "lib/ts/**/*" ],
